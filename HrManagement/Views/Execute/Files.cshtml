@model System.Collections.Generic.List<HrManagement.Controllers.ExecuteFiles>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ErrorLogs</title>
</head>
<body>
    <div>


        @for (int i = 0; i < Model.Count; i++)
        {

            <div><a target="_blank" href="@Url.Action("ShowText", "Execute", new { Path = @Model[i].FileName })">@Model[i].FileName</a> <p>Modifed at : @Model[i].Date</p></div>
            <br />
        }



        @Model.Count
    </div>
</body>
</html>
